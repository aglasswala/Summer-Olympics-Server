Login Table

create table login(loginid serial primary key, hash varChar(100) not null, email text unique not null);

Users table 

CREATE TABLE Users (userId serial PRIMARY KEY, FName varChar(20) not null, LName varChar(20) not null, Street varChar(255) not null, city varChar(20) not null, state char(2) not null, zip char(5) not null, email varChar(30) not null, phoneNumber char(10) not null, countryOfOrigin varChar(20) not null, userType int not null);
